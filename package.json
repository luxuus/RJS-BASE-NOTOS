{
  "name": "rjs-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "clines": "clines ./src",
    "cyclomatic-complexity": "cyclomatic-complexity './src/**/*.{ts,tsx}'",
    "postinstall": "npm run clines",
    "start": "vite",
    "dev:api": "json-server --watch ./data/db.json --port 5050",
